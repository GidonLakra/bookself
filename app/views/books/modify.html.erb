<%= form_with model: @book do |form| %>
    <%= form.label :title %>
        <%= form.text_field :title %>
    <%= form.label :author %>
        <%= form.text_field :author %>
    <%= form.label :description %>
        <%= form.text_field :description %>
     <%= form.label :rating %>
        <%= form.text_field :rating %>
    <%= form.label :reading_status %>
        <%= form.text_field :reading_status %>

    <%= form.submit %>
<% end %>
<button> <%= link_to "Back", books_path, method: :get  %> </button>